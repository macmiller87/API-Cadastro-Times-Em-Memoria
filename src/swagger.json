    "paths": {
        "/createUser": {
            "post": {
                "tags": ["Create"],
                "summary": "Create User",
                "description": "Create a New User",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string"
                                    },
                                    "userAvatar": {
                                        "type": "string"
                                    }
                                }
                            },
                            "example": {
                                "username": "username sample",
                                "userAvatar": "useravatar sample"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User Create With Sucess !"
                    },
                    "404": {
                        "description": "User Already Exists !"
                    }
                }
            }
        }
    }

